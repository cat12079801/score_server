<div class="row">
  <div class="col-lg-6">
    <table class="table table-striped table-hover ">
      <thead>
        <tr>
          <th>順位</th>
          <th>チーム</th>
          <th>得点</th>
        </tr>
      </thead>
      <tbody>
        <% @teams.each.with_index(1) do |team, i| %>
          <tr>
            <td><%= i %></td>
            <td><%= team[0] %></td>
            <td><%= team[1] %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>

  <div class="col-lg-6">
    <table class="table table-striped table-hover ">
      <thead>
        <tr>
          <th>順位</th>
          <th>アカウント</th>
          <th>チーム</th>
          <th>得点</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each.with_index(1) do |user, i| %>
          <tr>
            <td><%= i %></td>
            <td><%= user[1] %></td>
            <td><%= user[2] %></td>
            <td><%= user[0] %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
