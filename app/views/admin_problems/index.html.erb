<p id="notice"><%= notice %></p>

<h1>Listing Admin Problems</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_problems.each do |admin_problem| %>
      <tr>
        <td><%= link_to 'Show', admin_problem %></td>
        <td><%= link_to 'Edit', edit_admin_problem_path(admin_problem) %></td>
        <td><%= link_to 'Destroy', admin_problem, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin problem', new_admin_problem_path %>
